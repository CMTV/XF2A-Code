<phrases>
  <phrase title="CMTV_Code_code_block_highlight" version_id="2010070" version_string="2.1.0"><![CDATA[Highlight lines]]></phrase>
  <phrase title="CMTV_Code_code_block_highlight_explain" version_id="2010070" version_string="2.1.0"><![CDATA[Optional. Line numbers you want to highlight. For example, "2, 4-6" will highlight lines 2, 4, 5 and 6.]]></phrase>
  <phrase title="CMTV_Code_code_block_title" version_id="2010070" version_string="2.1.0"><![CDATA[Code block title]]></phrase>
  <phrase title="CMTV_Code_code_block_title_explain" version_id="2010070" version_string="2.1.0"><![CDATA[Optional. If specified, the default code language name will be replaced with title text at code block header. For example, can be used for specifying filename.]]></phrase>
  <phrase title="CMTV_Code_collapse" version_id="2010070" version_string="2.1.0"><![CDATA[Collapse]]></phrase>
  <phrase title="CMTV_Code_copied" version_id="2010070" version_string="2.1.0"><![CDATA[Code copied to clipboard.]]></phrase>
  <phrase title="CMTV_Code_copy" version_id="2010070" version_string="2.1.0"><![CDATA[Copy]]></phrase>
  <phrase title="CMTV_Code_expand" version_id="2010070" version_string="2.1.0"><![CDATA[Expand]]></phrase>
  <phrase title="option.CMTV_Code_backComp" version_id="2010070" version_string="2.1.0"><![CDATA[Code addon backwards compatibility]]></phrase>
